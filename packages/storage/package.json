  "devDependencies": {
    "@textile/crypto": "^2.0.0",
    "@types/chai-as-promised": "^7.1.3",
    "@types/event-emitter": "^0.3.3",
    "@types/node": "^14.14.11",
    "chai-as-promised": "^7.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.1",
    "ts-mockito": "^2.6.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@space/users": "^0.0.1",
    "@textile/hub": "^4.1.0",
    "event-emitter": "^0.3.5",
